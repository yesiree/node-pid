{
  "name": "@yesiree/pid",
  "version": "0.1.1",
  "description": "A module for generating universally-unique, public IDs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npm run build && jasmine",
    "format": "prettier --write 'src/**/*.ts' 'src/**/*.js'",
    "lint": "tslint -p tsconfig.json",
    "coverage": "nyc npm run test",
    "checks": "npm run coverage && npm run lint",
    "prepublishOnly": "npm run checks",
    "preversion": "npm run checks",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "deploy": "tsc && npm run checks && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yesiree/node-pid.git"
  },
  "keywords": [
    "uuid",
    "guid",
    "small",
    "compact",
    "id",
    "pid",
    "rfc4122"
  ],
  "author": "yesiree foundation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yesiree/node-pid/issues"
  },
  "homepage": "https://github.com/yesiree/node-pid#readme",
  "dependencies": {
    "big-integer": "^1.6.48",
    "nyc": "^15.0.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/big-integer": "0.0.31",
    "@types/uuid": "^3.4.6",
    "coveralls": "^3.0.9",
    "jasmine": "^3.5.0",
    "prettier": "^1.19.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  }
}
