{
  "name": "@yesiree/pid",
  "version": "1.0.0",
  "description": "A module for generating universally-unique, public IDs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "jasmine": "jasmine",
    "format": "prettier --write 'src/**/*.ts' 'src/**/*.js'",
    "lint": "tslint -p tsconfig.json",
    "coverage": "nyc npm run jasmine",
    "report-coverage": "nyc report --reporter=text-lcov > .ignore/coverage.lcov && codecov --disable=gcov",
    "test": "npm run build && npm run coverage && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yesiree/node-pid.git"
  },
  "keywords": [
    "uuid",
    "guid",
    "small",
    "compact",
    "id",
    "pid",
    "rfc4122"
  ],
  "author": "yesiree foundation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yesiree/node-pid/issues"
  },
  "homepage": "https://github.com/yesiree/node-pid#readme",
  "dependencies": {
    "big-integer": "^1.6.48",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/big-integer": "0.0.31",
    "@types/uuid": "^8.3.0",
    "codecov": "^3.8.2",
    "coveralls": "^3.1.0",
    "jasmine": "^3.5.0",
    "nyc": "^15.0.1",
    "prettier": "^2.3.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.4"
  }
}
